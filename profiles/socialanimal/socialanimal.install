<?php
/**
 * @file
 * Example install file.
 */

/**
 * Implementation of hook_install().
 */
function socialanimal_install() {
  theme_enable(array('seven', 'socialanimal'));
  $variables = array(
    'admin_theme' => 'seven',
    'node_admin_theme' => 1,
    'theme_default' => 'socialanimal',
  );
  foreach ($variables as $key => $variable) {
    variable_set($key, $variable);
  }
}
